{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "CorsPolicy": {
        "AllowedOrigins": [ "https://example.com", "https://anotherdomain.com" ],
        "AllowedMethods": [ "GET", "POST", "PUT", "DELETE" ],
        "AllowedHeaders": [ "Content-Type", "Authorization" ]
    }
}